{
  "name": "cs732-example-react-express",
  "version": "1.0.0",
  "private": true,
  "description": "React + Express example with npm workspaces",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently -c \"cyan.bold,magenta.bold\" \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build": "npm run build --workspace=frontend",
    "start": "npm run start --workspace=backend",
    "install:all": "npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.2.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
